{
  "name": "unplugin-jade-garden",
  "version": "1.0.4",
  "description": "Plugins for generating CSS with Jade Garden and Tailwind CSS",
  "keywords": [
    "tailwindcss",
    "tailwind",
    "class-variance-authority",
    "slots-variance-authority",
    "classes",
    "classname",
    "classnames",
    "css",
    "cva",
    "sva",
    "styles",
    "variants"
  ],
  "author": "Gregory Salinas <ags1130@yahoo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-jade-garden/jade-garden.git",
    "directory": "packages/plugins"
  },
  "bugs": {
    "url": "https://github.com/project-jade-garden/jade-garden/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "bun typecheck && tsdown",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsdown --watch",
    "format": "biome format --write .",
    "lint": "biome lint",
    "postrestore": "clean-package restore",
    "prepack": "bun clean-package",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "jade-garden": "workspace:*",
    "unplugin": "2.3.10",
    "unplugin-utils": "0.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@rspack/core": "1.5.8",
    "@tailwindcss/postcss": "4.1.16",
    "@tailwindcss/vite": "4.1.16",
    "clean-package": "catalog:",
    "css-loader": "7.1.2",
    "es-toolkit": "catalog:",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "rollup": "4.52.5",
    "rollup-plugin-postcss": "4.0.2",
    "tailwindcss": "catalog:",
    "tsdown": "0.15.9",
    "typescript": "catalog:",
    "vite": "catalog:",
    "webpack": "5.102.1"
  }
}
